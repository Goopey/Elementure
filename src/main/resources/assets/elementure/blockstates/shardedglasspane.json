{
  "multipart": [
    {
      "apply": {
        "model": "elementure:block/shardedglasspane_post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "elementure:block/shardedglasspane_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "elementure:block/shardedglasspane_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "elementure:block/shardedglasspane_side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "elementure:block/shardedglasspane_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "elementure:block/shardedglasspane_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "elementure:block/shardedglasspane_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "elementure:block/shardedglasspane_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "elementure:block/shardedglasspane_noside",
        "y": 270
      }
    }
  ]
}